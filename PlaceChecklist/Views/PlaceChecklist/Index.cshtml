@model IndexViewModel
@section Style {
    <link rel="stylesheet" href="/css/main.css?v=1">
}
<div class="main">
    <div class="centre">

        <div class="the-title-of-the-list-of-shares">

            <div>Заведения</div>
        </div>

        <div class="list-of-shares">

            @foreach (var establishment in Model.Establishments)
            {
                <div class="table-of-shares">

                    <div>Название: @establishment.Name</div>
                    <div>Адрес: @establishment.Address</div>
                    <div>Категория: @establishment.Category</div>
                    <div>Тег: @establishment.Tags</div>
                    <div>Описание: @establishment.Description</div>
                </div>
            }
        </div>

    </div>
</div>
